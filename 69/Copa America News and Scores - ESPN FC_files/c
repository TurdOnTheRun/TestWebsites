erCase()!="US"){clipUrl=clipUrl.replace(APPLICATION_GUIDS[businessUnit]["domestic"],APPLICATION_GUIDS[businessUnit]["international"]);}else{clipUrl=clipUrl.replace(APPLICATION_GUIDS[businessUnit]["international"],APPLICATION_GUIDS[businessUnit]["domestic"]);}}return clipUrl;};function Clip(options){options=options||{};this.id=options.id||null;this.guid=options.guid||null;this.ooyalaId=null;this.timeRestrictions={embargoDate:null,expirationDate:null};this.geoRestrictions={type:null,countries:[]};var posterImage=options.thumbnailURL;this.posterImages={full:{href:posterImage}};this.links={mobile:{href:null,omniHref:null}};}Clip.prototype.loadAPIClip=function(options){var deviceParam="desktop";if(DEVICE.isMobile){deviceParam="mobile";}else{if(DEVICE.isIpad||DEVICE.isAndroid){deviceParam="tablet";}}var endpoint="http://espn.go.com/videohub/video/util/getMinifiedClipJsonById?id="+options.id+"&cms="+this.cms+"&device="+deviceParam;return $.ajax({method:"GET",url:endpoint,success:this.clip.convertAPIClip.bind(this),error:this.handleErrors.bind(this),dataType:"json"});};Clip.prototype.convertAPIClip=function(obj){if(obj.videos&&obj.videos.length>0){var apiClip=obj.videos[0];this.clip.id=apiClip.id;this.clip.timeRestrictions.embargoDate=apiClip.timeRestrictions.embargoDate;this.clip.timeRestrictions.expirationDate=apiClip.timeRestrictions.expirationDate;if(!(this.clip&&this.clip.posterImages&&this.clip.posterImages.full&&this.clip.posterImages.full.href&&this.clip.posterImages.full.href.trim()!=="")){this.clip.posterImages.full.href=apiClip.posterImages.full.href;}if(typeof apiClip.geoRestrictions==="object"){this.clip.geoRestrictions.type=apiClip.geoRestrictions.type;this.clip.geoRestrictions.countries=apiClip.geoRestrictions.countries;}if(apiClip&&apiClip.links&&typeof apiClip.links.mobile==="object"){if(apiClip.links.mobile.href){this.clip.links.mobile.href=apiClip.links.mobile.href;}if(apiClip.links.mobile.omniHref){this.clip.links.mobile.omniHref=apiClip.links.mobile.omniHref;}}}else{$.publish("espn.video.unavailable",[this,"Clip not found"]);}};Clip.prototype.loadCerebroClip=function(options){var endpoint="http://espn.go.com/espn360/cerebro/util/getClipById?id="+options.cerebroId;return $.ajax({method:"GET",url:endpoint,success:this.clip.convertCerebroClip.bind(this),error:this.handleErrors.bind(this),dataType:"text"});};Clip.prototype.convertCerebroClip=function(text){var cerebroClip=JSON.parse(text.replace(/ObjectId\(|ISODate\(|\)/g,""));cerebroClip=cerebroClip[0];this.clip.guid=cerebroClip._id;this.clip.timeRestrictions.embargoDate=cerebroClip.videoRights.embargoDate;this.clip.timeRestrictions.expirationDate=cerebroClip.videoRights.expirationDate;if(!(this.clip&&this.clip.posterImages&&this.clip.posterImages.full&&this.clip.posterImages.full.href&&this.clip.posterImages.full.href.trim()!=="")){this.clip.posterImages.full.href=IMG_REF+cerebroClip.mezzaninePathImage;}var type=cerebroClip.videoRights.geoTargetType;if(type==="inclusion"){type="whitelist";}if(type==="exclusion"){type="blacklist";}this.clip.geoRestrictions.type=type;var countries=cerebroClip.videoRights.geoTargets;if(Object.prototype.toString.call(countries)==="[object Array]"){this.clip.geoRestrictions.countries=countries;}var businessUnit=getBusinessUnit(this.cms);if(typeof cerebroClip.syndicationDetails[businessUnit]==="object"){if(typeof cerebroClip.syndicationDetails[businessUnit].cms==="object"){this.clip.id=cerebroClip.syndicationDetails[businessUnit].cms.syndicationId;}if(typeof cerebroClip.syndicationDetails[businessUnit].brightcove==="object"){this.clip.links.mobile.href=cerebroClip.syndicationDetails[businessUnit].brightcove.url;}if(typeof cerebroClip.syndicationDetails[businessUnit].ooyala==="object"){this.clip.ooyalaId=cerebroClip.syndicationDetails[businessUnit].ooyala.syndicationId;}}};Clip.prototype.isExpired=function(){return(Date.parse(this.clip.timeRestrictions.expirationDate)<Date.parse(new Date().toGMTString()));};Clip.prototype.isEmbargoed=function(){return(Date.parse(this.clip.timeRestrictions.embargoDate)>Date.parse(new Date().toGMTString()));};Clip.prototype.isGeoRestricted=function(){var isGeoRestricted=false;var type=this.clip.geoRestrictions.type;var countries=this.clip.geoRestrictions.countries;if(countries.length>0){var currentCountry=GEO_LOCATION.country.isoCodeAlpha2;var country;if(!!currentCountry){currentCountry=currentCountry.toLowerCase();if(type==="whitelist"){isGeoRestricted=true;}else{if(type==="blacklist"){isGeoRestricted=false;}}for(var i=0,len=countries.length;i<len;i++){country=countries[i].toLowerCase();if(country===currentCountry){if(type==="whitelist"){isGeoRestricted=false;}else{if(type==="blacklist"){isGeoRestricted=true;}}break;}}}else{isGeoRestricted=true;}}return isGeoRestricted;};Clip.prototype.getRestrictions=function(){var response={};response.status="success";response.data="";if(this.clip.isExpired.call(this)){response.status="fail";response.message="This video is no longer available";}else{if(this.clip.isEmbargoed.call(this)){response.status="fail";response.message="This video is not yet available";}else{if(this.clip.isGeoRestricted.call(this)){response.status="fail";var isoCodeAlpha2=GEO_LOCATION.country.isoCodeAlpha2;response.message="This video is not authorized for your location";if(typeof isoCodeAlpha2==="string"){response.message+=" ("+isoCodeAlpha2.toUpperCase()+")";}}}}return response;};function OoyalaPlayer(options){options=options||{};this.setDefaults.call(this,options);this.adPlaying=false;this.adSet=options.adSet;this.eventHandlerName=options.eventHandlerName||"handleEvents";this.moduleParams={"espn-analytics":{},"freewheel-ads-manager":{},"espn-ui":{}};options.playType=options.autoPlay?"autoplay":"manual";this.buildModuleParams.call(this,options);var self=this;var doVideoPlayerInit=function(){if(!!options.id){$.when(function(){self.clip.loadAPIClip(options);}).then(function(){self.init();});}else{if(!!options.cerebroId){$.when(function(){self.clip.loadAPIClip(options);}).then(function(){self.init();});}else{self.init();}}};if(OOYALA_SETTINGS!==null){doVideoPlayerInit();}}OoyalaPlayer.prototype=new Player();OoyalaPlayer.prototype.init=function(){$.publish("espn.video.embed.begin");this.pcode=OOYALA_SETTINGS[this.cms].pcode;if(this.adSet){this.adSetCode=AD_SETS[this.pcode][this.adSet];}else{if(GEO_LOCATION.country&&GEO_LOCATION.country.isoCodeAlpha2&&GEO_LOCATION.country.isoCodeAlpha2.toUpperCase()!="US"){this.adSetCode=AD_SETS_BY_UNIT[getBusinessUnit(this.cms)]["international"];}else{this.adSetCode=AD_SETS_BY_UNIT[getBusinessUnit(this.cms)]["domestic"];}}this.playerBrandingId=OOYALA_SETTINGS[this.cms].playerBrandingId;var playerjs="//player.ooyala.com/player.js?";var params=[];if(this.clip.ooyalaId){params.push("embedCode="+this.clip.ooyalaId);}if(this.clip.id){params.push("externalId="+this.cms+":"+this.clip.id);}params.push("pcode="+this.pcode);params.push("adSetCode="+this.adSetCode);params.push("playerBrandingId="+this.playerBrandingId);params.push("callback="+this.eventHandlerName);params.push("hasModuleParams=1");params.push("height="+this.getPixelHeight.call(this));params.push("targetReplaceId="+this.targetReplaceId);params.push("width="+this.getPixelWidth.call(this));params.push("wmode=opaque");playerjs+=params.join("&");$.getScript(playerjs);$.publish("espn.video.playerjsReady",[playerjs]);var hackOut=setTimeout(function(){var $videoPlayer=$(".videoplayer");if($videoPlayer.hasClass("loading")){$videoPlayer.removeClass("loading");$videoPlayer.addClass("loaded");}},5000);debug.log("espn.video","ooyala script call",playerjs);};OoyalaPlayer.prototype.playVideo=function(){if(checkMemHasVideo()){this.$container.addClass("videoplayer-show");}try{this.playerElement.playMovie();}catch(e){}};OoyalaPlayer.prototype.pauseVideo=function(){try{this.playerElement.pauseMovie();}catch(e){}};OoyalaPlayer.prototype.switchVideo=function(options){options=options||{};this.pauseVideo.call(this);this.clip.loadAPIClip.call(this,options);