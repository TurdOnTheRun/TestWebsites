content")[0];this.setupGrid();this.addListeners();return this;};this.addListeners=function(){if(FC.isDesktop()||FC.isTablet()){$(".common-link").on("mouseover",this.commonLinkMouseOver);$(".common-link").on("mouseout",this.commonLinkMouseOut);$(FC).bind(FC.EVENT_RESIZE,this.resize);this.resize();}if(FC.isDesktop()){$(FC).bind(FC.EVENT_SCROLL,this.scroll);this.scroll();}if(FC.isMobile()){var a=this.html.topStoryRelatedLink;$(a).on("click",this.expandTopStoryClick);}};this.removeListeners=function(){$(FC).unbind(FC.EVENT_RESIZE,this.resize);$(FC).unbind(FC.EVENT_SCROLL,this.scroll);};this.commonLinkMouseOver=function(){$(this).closest(".common-link-root").addClass("hover");};this.commonLinkMouseOut=function(){$(this).closest(".common-link-root").removeClass("hover");};this.expandTopStoryClick=function(){var a=this.html.topStoryRelatedLink;if(a.innerHTML.match(/^Close /)){a.innerHTML=a.innerHTML.replace(/^Close /,"");var transform="translate3d(0, 0, 0)";FC.setTransform(this.html.headlinesDiv,transform);FC.setTransform(this.html.otherStoriesDiv,transform);}else{a.innerHTML=a.innerHTML.replace(/^/,"Close ");var ul=this.html.topStoryRelatedList;var transform="translate3d(0, "+$(ul).height()+"px, 0)";FC.setTransform(this.html.headlinesDiv,transform);FC.setTransform(this.html.otherStoriesDiv,transform);}return false;};this.setupGrid=function(){if(FC.isDesktop()){this.setupDesktopGrid();}};this.setupDesktopGrid=function(){this.dates=$(".grid-item-date").get();if(this.dates.length===0){return;}var startIndex=0;$("#other-stories .grid-item").each(function(){if($(this).hasClass("grid-item-date")){startIndex=0;}else{if(startIndex%3==0){$(this).addClass("desktop-clear");}startIndex++;}});$(this.dates).each(self.updateDateTop);};this.updateDateTop=function(i,element){if($(element).next().length==1){var topOfDate=$(element).next().position().top,offsetOfDate=$(element).next().offset().top;$(element).css({top:topOfDate+"px",position:"absolute",left:"0px"});this.dateOffsets[i]=offset