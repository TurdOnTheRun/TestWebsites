.$container=$container;if(checkMemHasVideo()){this.$container.addClass("videoplayer-show");}this.targetElement=document.getElementById(this.targetReplaceId);$.publish("espn.video.container.ready",[this.$container]);$.publish("espn.video.targetReady",[this.targetReplaceId]);this.playerElement=null;this.$playerElement=null;this.onErrorBehavior=options.onErrorBehavior||"default";if(typeof options.useExternalIdFilter!=="undefined"){this.useExternalIdFilter=options.useExternalIdFilter;}else{this.useExternalIdFilter=CMS_INFO[this.cms].useExternalIdFilter;}};Player.prototype.cleanDimension=function(dim){if(dim=="default"){return null;}else{if(dim){dim=dim+"";return/\d{2,3}%/.test(dim)?dim:dim.replace(/\D/g,"")+"px";}}return null;};Player.prototype.handleErrors=function(message){$.publish("espn.video.unavailable",[this,message]);if(this.onErrorBehavior==="message"){this.displayError.call(this,message);}else{if(this.onErrorBehavior==="image"){var w=(this.width=="default")?"":this.width;var h=(this.height=="default")?"":this.height;$("#"+this.targetReplaceId).replaceWith('<img id="'+this.targetReplaceId+'" src="'+this.clip.posterImages.full.href+'" width="'+w+'" height="'+h+'" />');}else{if(this.onErrorBehavior==="hide"){$("#"+this.targetReplaceId).hide();}}}};Player.prototype.displayError=function(message){message=(typeof message==="string")?message+"<br />":"";var w=(this.width=="default")?"":this.width;var h=(this.height=="default")?"":this.height;var $container=$('<div id="'+this.targetReplaceId+'" class="imageHolder"></div>').css("position","relative");var $imgObj=$('<img src="'+this.clip.posterImages.full.href+'" width="'+w+'" height="'+h+'">');$container.append($imgObj);var $messObj=$('<div class="message">CONTENT UNAVAILABLE: '+message+"</div>").css({"font-family":"klavika, arial, sans-serif","font-size":"24px","text-decoration":"none","text-align":"center",position:"absolute",width:"100%",top:"50%",color:"#FFF","text-shadow":"black 0.1em 0.1em 0.2em"});$container.app