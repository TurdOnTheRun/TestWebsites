eady){changeType=this.playType==="continuous"?"endCard":"switch";}else{this.playerReady=true;changeType="init";espn.video.player.ready=true;$.publish("espn.video.ready",[params,playerId,eventName,"espn.video.ready"]);$.publish("espn.video.init",[params,playerId,eventName,"espn.video.init"]);this.playerElement.onmouseover=function(){$.publish("espn.video.hoverIn");};this.playerElement.onmouseout=function(){$.publish("espn.video.hoverOut");};}$.publish("espn.video.embedCodeChanged",[changeType]);break;case"autostartDisabled":this.setAutoPlay.call(this,false);$.publish("espn.video.autostart.disabled",[params,playerId,eventName]);break;case"autostartEnabled":this.setAutoPlay.call(this,true);$.publish("espn.video.autostart.enabled",[params,playerId,eventName]);break;case"channelPlayComplete":break;case"endCardActive":break;case"endCardInactive":break;case"playComplete":this.playType="continuous";$.publish("espn.video.complete",[params,playerId,eventName]);break;case"playerEmbedded":this.playerElement=document.getElementById(this.targetReplaceId);this.setModuleParams.call(this);break;case"playheadTimeChanged":if(this.playVerified===false&&this.adPlaying===false){this.playVerified=true;$.publish("espn.video.contentStarted",[this.clip.id,this.playType]);}$.publish("espn.video.playheadTimeChanged",[params,playerId,eventName]);break;case"seeked":$.publish("espn.video.seeked",[params,playerId,eventName]);break;case"shareMenuActive":break;case"shareMenuInActive":break;case"startScreenThumbnailReady":$.publish("espn.video.thumbnailReady",[params,playerId,eventName]);break;case"stateChanged":if(params.state==="error"){var $videoPlayer=$(".videoplayer");if($videoPlayer.hasClass("loading")){$videoPlayer.removeClass("loading");$videoPlayer.addClass("loaded");}this.handleErrors.call(this);$.publish("espn.video.error",[params,playerId]);}else{if(params.state==="paused"&&this.isPlaying){this.isPlaying=false;$.publish("espn.video.pause",[params,playerId,eventName]);}else{if(params.state==="playing"&&this.isPlaying===false){espn.refresh.stop();this.isPlaying=true;$.publish("espn.video.play",[params,playerId,eventName]);}}}break;case"volumeChanged":$.publish("espn.video.volumeChanged",[params,playerId,eventName]);break;}};OoyalaPlayer.prototype.skipAd=function(){try{this.playerElement.skipAd();}catch(e){}};OoyalaPlayer.prototype.handleErrors=function(message){$.publish("espn.video.unavailable",[this,message]);if(this.onErrorBehavior==="image"){$("#"+this.targetReplaceId).replaceWith('<img src="'+this.clip.posterImages.full.href+'" width="'+this.width+'" height="'+this.height+'" />');}else{if(this.onErrorBehavior==="hide"){this.$container.css({height:"0px",overflow:"hidden"});}}};OoyalaPlayer.prototype.getPlayType=function(){};OoyalaPlayer.prototype.buildModuleParams=function(options){options=options||{};var names,name;if(typeof options.autostart!=="undefined"){options.autoPlay=options.autostart;}else{if(typeof options.autoplay!=="undefined"){options.autoPlay=options.autoplay;}}if(typeof options.FRMSegment==="undefined"){options.FRMSegment=AD_SEGMENTS();}if(typeof options.siteSection==="string"){options.fw_site_section_id=options.siteSection;}else{if(typeof options.fw_site_section_id==="string"){options.siteSection=options.fw_site_section_id;}}if(typeof options.thumbnailURL==="string"){options.thumbnail=options.thumbnailURL;}if(typeof options.trackingName==="string"){options.playerType=options.trackingName;}else{if(typeof options.playerType==="string"){options.trackingName=options.playerType;}else{if(typeof options.player==="string"){options.playerType=options.player;options.trackingName=options.player;}}}names=["league","localSite","omnitureAccount","playerType","playerTypePartner","playerTypePartnerAuthor","playType","siteReportName","SWID","trackingName"];for(var i=0,len=names.length;i<len;i++){name=names[i];if(typeof options[name]!=="undefined"){this.moduleParams["espn-analytics"][name]=options[name]+"";}}names=["adUnit","FRMSegment","fw_site_sec