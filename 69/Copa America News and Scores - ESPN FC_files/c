);return;}else{if(e.touches.length==1){var touch=e.touches[0];ex=touch.pageX;ey=touch.pageY;}}}else{ex=e.clientX;ey=e.clientY;}this.velocityX=0;this.mouseDownX=ex;this.mouseDownY=ey;this.mouseDownAt=new Date();if(FC.isMobile()){window.ontouchmove=this.mouseMoveHandler;window.ontouchend=this.mouseUpHandler;}else{document.body.className+=" unselectable dragging slideshowdrag";document.onselectstart=function(){return false;};document.onmousemove=this.mouseMoveHandler;document.onmouseup=this.mouseUpHandler;}};this.mouseMove=function(e){e=e||window.event;var ex,ey;if(e.touches){if(e.touches.length>1){this.stopDragging();return;}else{if(e.touches.length==1){var touch=e.touches[0];ex=touch.pageX;ey=touch.pageY;}}}else{ex=e.clientX;ey=e.clientY;}if(!this.dragging){if(FC.isMobile()){this.startDragging();return true;}var draggedX=Math.abs(ex-this.mouseDownX);var draggedY=Math.abs(ey-this.mouseDownY);if(draggedY>10){this.stopDragging();}else{if(draggedX>15){this.startDragging();}}return true;}this.finalX=-(this.mouseDownX-this.startX-ex);this.finalY=-(this.mouseDownY-this.startY-ey);if(this.finalX>this.boundaryRight){var d=this.finalX-this.boundaryRight;this.finalX=this.boundaryRight+(d/3);}else{if(this.finalX<this.boundaryLeft){var d=this.finalX-this.boundaryLeft;this.finalX=this.boundaryLeft+(d/3);}}this.velocityX=ex-this.lastX;this.velocityY=ey-this.lastY;this.lastX=ex;this.lastY=ey;return false;};this.mouseUp=function(e){if(!this.dragging){this.stopDragging();this.tap();return;}if(this.velocityX>0){this.finalX=Math.ceil(this.offsetX/this.itemWidth)*this.itemWidth;}else{this.finalX=Math.floor(this.offsetX/this.itemWidth)*this.itemWidth;}if(this.finalX<this.boundaryLeft){this.finalX=this.boundaryLeft;}else{if(this.finalX>this.boundaryRight){this.finalX=this.boundaryRight;}}this.updatePagination();this.stopDragging();};this.tap=function(){if(this.mouseDownTarget){for(var i=0;i<this.items.length;i++){var item=this.items[i];if(item==this.mouseDownTarget){if(this.index==i){item.click();return;}this.finalX=i*this.itemWidth;if(this.finalX>this.boundaryRight){this.finalX=this.boundaryRight;}this.updatePagination();this.startAnimating();return;}}}};this.startDragging=function(){this.startX=this.offsetX;this.startY=this.offsetY;this.finalX=this.offsetX;this.finalY=this.offsetY;this.velocityX=0;this.velocityY=0;if(FC.isMobile()){window.ontouchmove=this.mouseMoveHandler;window.ontouchend=this.mouseUpHandler;}else{document.onmousemove=this.mouseMoveHandler;document.onmouseup=this.mouseUpHandler;}this.startAnimating();this.dragging=true;};this.stopDragging=function(){this.dragging=false;if(FC.isMobile()){window.ontouchmove=null;window.ontouchend=null;}else{document.onmousemove=null;document.onmouseup=null;}if(!FC.isMobile()){document.onselectstart=null;$(document.body).removeClass("dragging unselectable slideshowdrag");}for(var i=0;i<this.items.length;i++){var item=this.items[i];if(item.pauseVideo){item.pauseVideo();}}};this.startAnimating=function(){if(this.animating){return;}$(FC).bind(FC.RENDER_FRAME,this.animateFrameHandler);this.animating=true;};this.stopAnimating=function(){if(!this.animating){return;}$(FC).unbind(FC.RENDER_FRAME,this.animateFrameHandler);this.animating=false;};this.animateFrame=function(e){var k=0.16;var x=this.finalX-this.offsetX;var f=k*x;if(!this.dragging&&Math.abs(f)<0.01){if(this.loadMe){this.loadMe.load();}this.offsetX=Math.round(this.finalX);this.stopAnimating();this.updateCSS();return;}this.offsetX+=f;this.updateCSS();};this.updatePagination=function(){this.index=Math.ceil(-this.finalX/this.itemWidth);this.pagination.selectPageAtIndex(this.index);};this.updateCSS=function(){this.div.style.msTransform="translate("+this.offsetX+"px)";this.div.style.MozTransform="translate("+this.offsetX+"px)";this.div.style.webkitTransform="translate3d("+this.offsetX+"px,0,0)";if(FC.IE&&FC.IE<9){this.div.style.marginRight=-this.offsetX+"px";}};this.gotoPage=function(index){this.index=index;this.finalX=index*this.itemWidth;if(this.final