params,this.targetReplaceId,eventName]);break;case"error":this.handleErrors.call(this);$.publish("espn.video.error",[params,this.targetReplaceId]);break;case"loadstart":this.playerElement=document.getElementById(this.targetReplaceId);this.playerElement.onmouseover=function(){$.publish("espn.video.hoverIn");};this.playerElement.onmouseout=function(){$.publish("espn.video.hoverOut");};break;case"pause":this.isPlaying=false;$.publish("espn.video.pause",[params,this.targetReplaceId,eventName]);break;case"play":this.isPlaying=true;if(this.clip&&this.clip.links&&this.clip.links.mobile&&this.clip.links.mobile.omniHref){var omniHref=this.clip.links.mobile.omniHref;if(typeof s_omni!=="undefined"&&typeof s_omni.visitor!=="undefined"){if(s_omni.visitor.getAnalyticsVisitorID()){var vid="&vid=%5BCS%5Dv1%7C"+s_omni.visitor.getAnalyticsVisitorID()+"%5BCE%5D&";omniHref=omniHref+vid;$.ajax({method:"GET",cache:false,url:omniHref,success:function(){},error:function(){},dataType:"json"});}else{var trackTimeout=setTimeout(function(){if(s_omni.visitor.getAnalyticsVisitorID()){var vid="&vid=%5BCS%5Dv1%7C"+s_omni.visitor.getAnalyticsVisitorID()+"%5BCE%5D&";omniHref=omniHref+vid;$.ajax({method:"GET",cache:false,url:omniHref,success:function(){},error:function(){},dataType:"json"});}},2000);}}}$.publish("espn.video.play",[params,this.targetReplaceId,eventName]);break;case"seeked":$.publish("espn.video.seeked",[params,this.targetReplaceId]);break;case"timeupdate":$.publish("espn.video.playheadTimeChanged",[params,this.targetReplaceId,eventName]);break;case"volumechange":$.publish("espn.video.volumeChanged",[params,this.targetReplaceId,eventName]);break;}};HTML5VideoPlayer.prototype.registerEvents=function(){var events=["abort","canplay","canplaythrough","durationchange","emptied","ended","error","loadeddata","loadedmetadata","loadstart","mozaudioavailable","play","pause","playing","progress","ratechange","seeked","seeking","stalled","suspend","timeupdate","volumechange","waiting"];for(var i=0