.julyOffset==630?"LHT":"ET",690:"NFT"},bd:{360:"BST"},bo:{240:"BOT"},br:{120:this.todaysOffset==120?"BRST":"BRT",180:"BRT",240:"AMT"},ca:{210:"NT",240:"AT",300:"ET",360:"CT",420:"MT",480:"PT"},cl:{180:"CLT"},cn:{480:"CST"},co:{300:"COT"},cr:{360:"CST"},de:{60:this.todaysOffset==60?"CET":"CEST"},"do":{240:"AST"},ec:{300:"ECT"},es:{60:"CET"},fi:{120:this.todaysOffset==120?"EET":"EEST"},gt:{360:"CST"},gb:{0:this.todaysOffset==this.januaryOffset?"GMT":"BST"},gh:{0:"GMT"},hn:{360:"CST"},hk:{480:"HKT"},"in":{330:"IST"},id:{420:"WIB",480:"WITA",540:"WIT"},ir:{0:this.todaysOffset==0?"GMT":"IST"},jm:{300:"EST"},jp:{540:"JST"},ke:{180:"EAT"},kr:{540:"KST"},mx:{360:"CT",420:"MST"},my:{480:"MYT"},mu:{240:"MUT"},ni:{360:"CST"},ng:{60:"WAT"},nz:{780:"NZT"},pa:{300:"EST"},pk:{300:"PKT"},pe:{300:"PET"},pr:{240:"AST"},pt:{0:"WET"},sg:{480:"SGT"},sv:{360:"CST"},za:{120:"SAT"},se:{60:this.todaysOffset==60?"CET":"CEST"},tz:{180:"EAT"},th:{420:"ICT"},tt:{240:"AST"},ug:{180:"EAT"},uy:{120:"UYT"},uk:{0:this.todaysOffset==this.januaryOffset?"GMT":"BST"},us:{300:"ET",360:"CT",420:this.julyOffset==420?"MST":"MT",480:"PT",540:"AKT",600:"HAST"},ve:{270:"VET"},defaults:{0:this.todaysOffset==this.januaryOffset?"GMT":"BST",120:this.todaysOffset==120?"BRST":"BRT",180:"BRT",240:"AMT",300:"ET",360:"CT",420:this.julyOffset==420?"MST":"MT",480:"PT"}};};TimeZone.prototype.updateCountry=function(){$.getScript("http://tredir.go.com/capmon/GetDE?set=j&param=country&param=countryisoalpha2code").done(function(){if(!!countryisoalpha2code){if(countryisoalpha2code!=this.country){var cookieDate=new Date();cookieDate.setTime(cookieDate.getTime()+(7*24*60*60*1000));$.cookie("country",null,{expires:-1});$.cookie("country",countryisoalpha2code,{domain:"",expires:cookieDate});}}});};TimeZone.prototype.init=function(){var today=new Date();var year=today.getFullYear();var januaryFirst=new Date(year,0,1,0,0,0,0);var julyFirst=new Date(year,6,1,0,0,0,0);this.januaryTotalOffset=januaryFirst.getTimezoneOffset();this.januaryOffset=Math.abs(this.januaryTotalOffset);this.julyOffset=Math.abs(julyFirst.getTimezoneOffset());this.todaysOffset=Math.abs(today.getTimezoneOffset());this.country=this.getCountry().toLowerCase();this.initTimeZones();var countryTimezones=this.timezones[this.country];if(!!countryTimezones){var abbrev=countryTimezones[this.januaryOffset];if(!!abbrev){this.abbrev=abbrev;}else{if(!!this.timezones.defaults[this.januaryOffset]){this.abbrev=this.timezones.defaults[this.januaryOffset];this.updateCountry();}else{this.updateCountry();}}}else{if(!!this.timezones.defaults[this.januaryOffset]){this.abbrev=this.timezones.defaults[this.januaryOffset];this.updateCountry();}else{this.updateCountry();}}return this;};TimeZone.prototype.setTime=function(div,year,month,date,hour,minute){var jDiv=$(div),selectedDate=$("#selected-date").data("date");if(!!jDiv&&jDiv.length>0){if(typeof year=="number"&&year==year&&typeof month=="number"&&month==month&&typeof date=="number"&&date==date&&typeof hour=="number"&&hour==hour&&typeof minute=="number"&&minute==minute){d=new Date();var dYear=d.getFullYear(),dMonth=d.getMonth()+1,dDate=d.getDate(),todayYearMonthDay=""+dYear+(dMonth<10?"0"+dMonth:dMonth)+(dDate<10?"0"+dDate:dDate);d.setUTCSeconds(0);d.setUTCMinutes(minute);d.setUTCHours(hour);d.setUTCDate(date);d.setUTCMonth(month-1);d.setUTCFullYear(year);jDiv.html(d.toLocaleTimeString().replace(/(\d{1,2})\:(\d{1,2})(?:\d*)(?:\:(?:\d*))?[\s]*(AM|PM)?(?:[\s\S]*)/,"$1:$2 $3")+" "+this.abbrev);dYear=d.getFullYear();dMonth=d.getMonth()+1;dDate=d.getDate();var gameYearMonthDay=""+dYear+(dMonth<10?"0"+dMonth:dMonth)+(dDate<10?"0"+dDate:dDate);if(!!selectedDate&&selectedDate.toString()!=gameYearMonthDay&&jDiv.closest("#scorebar").length==0){if(jDiv.parent().find(".game-date").length>0){jDiv.parent().find(".game-date").html(""+this.days[d.getDay()]+", "+this.months[dMonth-1]+" "+dDate);}else{jDiv.after('<span class="game-date">'+this.days[d.getDay()]+", "+this.months[dMonth-1]+" "+dDate+"</span>");}}else{i