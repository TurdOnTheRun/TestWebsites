r.bind(this);};FC.SortableTable=function(){var self=this;this.html={table:null,tbody:null,rows:null,columnRow:null,headers:null};this.columns=[];this.init=function(table){var jtable=$(table);this.html.table=table;this.html.tbody=jtable.find("tbody")[0];this.html.rows=jtable.find("tbody tr").toArray();this.html.columnRow=jtable.find("tr.columns")[0];this.html.headers=jtable.find("tr.columns th").toArray();for(var i=0;i<this.html.headers.length;i++){var header=this.html.headers[i];var sortable=header.getAttribute("data-sort-by");if(sortable){var sortableColumn=new FC.SortableTableColumn().init(header,i);self.columns.push(sortableColumn);sortableColumn.delegate=self;}}return this;};var intVal=function(val){val=parseInt(val,10);return isNaN(val)?(-1*Infinity):val;};var numSort=function(a,b){var aValue=intVal(a.value),bValue=intVal(b.value);if(aValue===bValue&&a.cells.length>0&&b.cells.length>0){aValue=intVal($(a.cells[0]).text());bValue=intVal($(b.cells[0]).text());return bValue-aValue;}return aValue-bValue;};var invalidAlphaValue="zzzzzz";invalidAlphas={"N/A":invalidAlphaValue,"--":invalidAlphaValue};var alphaSort=function(a,b){var textA=a.value.toUpperCase(),textB=b.value.toUpperCase();if(textA in invalidAlphas){textA=invalidAlphas[textA];}if(textB in invalidAlphas){textB=invalidAlphas[textA];}return(textA<textB)?-1:(textA>textB)?1:0;};this.columnClick=function(column){var columnIndex=$.inArray(column,this.columns);this.delegate.sortByColumn(this,column,columnIndex);};this.sortByColumn=function(column){var rows=[];this.html.rows=$(this.html.table).find("tbody tr").toArray();for(var i=0;i<this.html.rows.length;i++){var row=this.html.rows[i];if(row==this.html.columnRow){continue;}var cell=$(row).find("td")[column.cell];row.value=cell.getAttribute("data-value")||$(cell).text();rows.push(row);}if(column.sort=="ALPHA"){rows.sort(alphaSort);}else{if(column.sort=="NUM"){rows.sort(numSort);}}if(column.ascending){rows.reverse();}for(var j=0;j<rows.length;j++){var row=rows[j];row.parentNode.removeChild(row);}for(var k=0;k<rows.length;k++){var row=rows[k];this.html.tbody.appendChild(row);}for(var l=0;l<this.columns.length;l++){var otherColumn=this.columns[l];if(otherColumn==column){continue;}otherColumn.setSelected(false);}column.ascending=!column.ascending;column.setColumnClass();column.setSelected(true);};};FC.SortableTable.SORT_ALPHA="ALPHA";FC.SortableTable.SORT_NUM="NUM";FC.SortableTableColumn=function(){var self=this;this.sort=null;this.cell=null;this.delegate=null;this.ascending=true;this.html={th:null,a:null};this.init=function(th,index){this.html.th=th;this.html.a=$(th).find("a")[0];var sortValues=th.getAttribute("data-sort-by").split(",");this.ascending=(sortValues[1].toUpperCase()=="ASC");this.sort=sortValues[0].toUpperCase();this.cell=index;this.addListeners();return this;};this.addListeners=function(){this.html.a.onclick=this.click;};this.click=function(){this.delegate.columnClick(this);return false;};this.setColumnClass=function(){jth=$(this.html.th);jth.removeClass("asc desc");this.ascending?jth.addClass("asc"):jth.addClass("desc");};this.setSelected=function(selected){if(selected){$(this.html.th).addClass("selected");}else{$(this.html.th).removeClass("selected");}};this.click=this.click.bind(this);};FC.ResponsiveTable=function(){var self=this;this.html={div:null,areaDiv:null,cropDiv:null,contentDiv:null,table:null,tableBody:null,tableHead:null,tableCopyDiv:null,tableCopy:null,firstRow:null,firstRowCells:null,shadowDiv:null,shadowDivContent:null};this.fixedColumns=null;this.sortableTable=null;this.init=function(div){var jDiv=$(div);this.html.div=div;this.html.areaDiv=jDiv.find(".responsive-table-area")[0];this.html.cropDiv=jDiv.find(".responsive-table-crop")[0];this.html.contentDiv=jDiv.find(".responsive-table-content")[0];this.html.table=jDiv.find("table").get(0);this.html.tableBody=jDiv.find("table tbody").get(0);this.html.tableHead=jDiv.find("table thead").get(0);this.html.firstRow=$(this.html.tableBody).find("tr").toArray();this.html.firstRowCells=$(this.html.firstRow).find("td").toArray();this.fixedColumns=this.html.table.getAttribute("data-fixed-columns")*1;var tableCopy=$(this.html.table).clone()[0];tableCopy.style.width=this.html.table.offsetWidth+"px";var tableCopyDiv=document.createElement("div");tableCopyDiv.className="responsive-table-copy";tableCopyDiv.appendChild(tableCopy);var leftShadowDiv=document.createElement("div");leftShadowDiv.className="responsive-table-shadow left";var rightShadowDiv=document.createElement("div");rightShadowDiv.className="responsive-table-shadow right";this.html.leftShadowDiv=leftShadowDiv;this.html.rightShadowDiv=rightShadowDiv;this.html.tableCopyDiv=tableCopyDiv;this.html.tableCopy=tableCopy;this.sortableTable=new FC.SortableTable().init(this.html.table);this.sortableTable.delegate=this;this.sortableTableCopy=new FC.SortableTable().init(tableCopy,true);this.sortableTableCopy.delegate=this;this.html.div.appendChild(leftShadowDiv);this.html.div.appendChild(rightShadowDiv);this.html.div.appendChild(tableCopyDiv);this.hideFixedColumns();this.addListeners();return this;};this.recreateTable=function(){$(this.html.tableCopy).remove();var tableCopy=$(this.html.table).clone()[0];tableCopy.style.width=this.html.table.offsetWidth+"px";this.html.tableCopyDiv.appendChild(tableCopy);this.html.tableCopy=tableCopy;this.sortableTableCopy=new FC.SortableTable().init(tableCopy,true);this.sortableTableCopy.delegate=this;this.resize();};this.sortByColumn=function(sortableTable,column,columnIndex){if(sortableTable==this.sortableTableCopy){column=this.sortableTable.columns[columnIndex];}this.sortableTable.sortByColumn(column);this.recreateTable();return false;};this.addListeners=function(){$(this.html.cropDiv).on("scroll",this.scroll);$(window).on("resize",this.resize);$(window).on("load",this.resize);};this.removeListeners=function(){$(this.html.cropDiv).unbind("scroll",this.scroll);$(window).unbind("resize",this.resize);$(window).unbind("load",this.resize);};this.scroll=function(){clearTimeout(this.scrollTimeout);this.scrollTimeout=setTimeout(this.shadowCheck,150);};this.shadowCheck=function(){if(this.html.cropDiv.scrollLeft>0){$(this.html.div).addClass("scrolled");}else{$(this.html.div).removeClass("scrolled");}};this.hideFixedColumns=function(){var rows=$(this.html.tableBody).find("tr").toArray();for(var i=0;i<rows.length;i++){var row=rows[i];var tds=$(row).find("td").toArray().slice(0,this.fixedColumns);var ths=$(row).find("th").toArray().slice(0,this.fixedColumns);var cells=tds.concat(ths);for(var j=0;j<cells.length;j++){var cell=cells[j];var colspan=cell.getAttribute("colspan");if(colspan){j+=parseInt(colspan);}$(cell).addClass("hide");}}};this.getFixedWidth=function(){var fixedWidth=0;for(var i=0;i<this.fixedColumns;i++){if(!!this.html.firstRowCells&&!!this.html.firstRowCells[i]){fixedWidth+=this.html.firstRowCells[i].offsetWidth;}}return fixedWidth;};this.resize=function(){var fixedWidth=this.getFixedWidth();var tableWidth=this.html.table.offsetWidth;var tableHeight=this.html.table.offsetHeight;if(!tableWidth||!tableHeight){return;}this.html.tableCopy.style.width=tableWidth+"px";this.html.tableCopy.style.height=tableHeight+"px";this.html.tableCopyDiv.style.width=fixedWidth+"px";this.html.tableCopyDiv.style.height=tableHeight+"px";this.html.leftShadowDiv.style.left=fixedWidth+"px";if(this.html.div.offsetWidth<this.html.table.offsetWidth){$(this.html.div).addClass("scrollable");}else{$(this.html.div).removeClass("scrollable");$(this.html.div).addClass("not-scrollable");}this.html.leftShadowDiv.style.height=this.html.tableBody.offsetHeight+"px";this.html.rightShadowDiv.style.height=this.html.tableBody.offsetHeight+"px";var offsetHeight="0px";if(!!this.html.tableHead){offsetHeight=this.html.tableHead.offsetHeight+"px";}this.html.leftShadowDiv.style.top=offsetHeight;this.html.rightShadowDiv.style.top=offsetHeight;};this.resize=this.resize.bind(this);this.scroll=this.scroll.bind(this);this.shadowCheck=this.shadowCheck.bind(